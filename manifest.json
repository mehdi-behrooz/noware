{
  "manifest_version": 2,

  "name": "Noware",
  "description": "Hides Yesware toolbars.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
        {
            "matches": ["https://mail.google.com/*"], 
            "css" : ["gmail.css"]
        }
  ]
}
